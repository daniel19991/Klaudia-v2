{
  "hosting": {
    "public":
         "/",
       
     
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
         "source": "**/!(*.css|*.js|*.map|*.jpg|*.gif|*.png|*.php|*.svg)",
        "destination": "/index.html"
      }
    ]
  }
}
