{
  "hosting": {
    "headers": [
      {
        "key": "strict-transport-security",
        "value": "max-age=31536000; includeSubDomains; preload"
      }
    ]
    "public":
         "/",
       
     
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
         "source": "**/!(*.css|*.js|*.map|*.jpg|*.gif|*.png|*.php)",
        "destination": "/index.html"
      }
    ]
  }
}
